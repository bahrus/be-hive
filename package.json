{
  "name": "be-hive",
  "version": "0.0.216",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements"
  ],
  "description": "Specify that ShadowDOM should inherit be-haviors from parent Shadow DOM",
  "main": "be-hive.js",
  "module": "be-hive.js",
  "exports": {
    ".": {
      "default": "./be-hive.js",
      "types": "./be-hive.ts"
    },
    "./aggEvt.js": {
      "default": "./aggEvt.js",
      "types": "./aggEvt.ts"
    },
    "./be-hive.js": {
      "default": "./be-hive.js",
      "types": "./be-hive.ts"
    },
    "./prsObj.js": {
      "default": "./prsObj.js",
      "types": "./prsObj.ts"
    },
    "./Registry.js": {
      "default": "./Registry.js",
      "types": "./Registry.js"
    }
  },
  "files": [
    "*.js",
    "*.ts"
  ],
  "scripts": {
    "serve": "node node_modules/may-it-serve/serve.js",
    "update": "ncu -u && npm install"
  },
  "dependencies": {
    "be-enhanced": "0.0.140",
    "mount-observer": "0.0.36",
    "trans-render": "0.0.835"
  },
  "devDependencies": {
    "may-it-serve": "0.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-hive.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-hive/issues"
  },
  "homepage": "https://github.com/bahrus/be-hive#readme"
}
