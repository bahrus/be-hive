{
  "name": "be-hive",
  "version": "0.0.225",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements"
  ],
  "description": "Specify that ShadowDOM should inherit be-hiviors from the parent Shadow DOM",
  "main": "be-hive.js",
  "module": "be-hive.js",
  "exports": {
    ".": {
      "default": "./be-hive.js",
      "types": "./be-hive.ts"
    },
    "./aggEvt.js": {
      "default": "./aggEvt.js",
      "types": "./aggEvt.ts"
    },
    "./be-hive.js": {
      "default": "./be-hive.js",
      "types": "./be-hive.ts"
    },
    "./prsObj.js": {
      "default": "./prsObj.js",
      "types": "./prsObj.ts"
    },
    "./Registry.js": {
      "default": "./Registry.js",
      "types": "./Registry.ts"
    },
    "./w.js": {
      "default": "./w.js",
      "types": "./w.ts"
    }
  },
  "files": [
    "*.js",
    "*.ts"
  ],
  "scripts": {
    "serve": "python3 ./node_modules/ssi-server/ssi_server.py",
    "update": "ncu -u && npm install"
  },
  "dependencies": {
    "be-enhanced": "0.0.147",
    "mount-observer": "0.0.39",
    "trans-render": "0.0.848"
  },
  "devDependencies": {
    "ssi-server": "0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-hive.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-hive/issues"
  },
  "homepage": "https://github.com/bahrus/be-hive#readme"
}
